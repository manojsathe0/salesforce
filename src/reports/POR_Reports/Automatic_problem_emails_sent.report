    <description>The total number of automatic problem emails sent, the number of problem cases that have received 1 automatic email, the number of problem cases that have received 2 automatic emails and the average number of automatic emails sent per day.</description>
    <filter>
        <criteriaItems>
            <column>Case$RecordType</column>
            <isUnlocked>false</isUnlocked>
            <operator>equals</operator>
            <value>Case.Problem_Case</value>
        </criteriaItems>
        <criteriaItems>
            <column>Case$Product2__c</column>
            <isUnlocked>false</isUnlocked>
            <operator>equals</operator>
