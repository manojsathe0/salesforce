        </criteriaItems>
        <criteriaItems>
            <column>Case.Activities$Activity_Type__c</column>
            <isUnlocked>false</isUnlocked>
            <operator>equals</operator>
            <value>Email</value>
        </criteriaItems>
        <criteriaItems>
            <column>Case$Total_Automatic_Outbound_Emails__c</column>
            <isUnlocked>false</isUnlocked>
            <operator>greaterThan</operator>
